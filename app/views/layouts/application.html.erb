<!DOCTYPE html>
<html>
<head>
  <title>Movie Rental</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<h1> All Wood Movie Rentals </h1>
<section id="flash_msg">
  <% flash.each do |key, value| %>
    <%= content_tag(:div,value,:class => "flash #{key}") %>
  <% end %>  
</section>
<%= yield %>
</body>
</html>
